n  const onNodeDragStop = (event, node) => {\n    dispatch({ type: 'UPDATE_QUESTION', payload: node })\n  }\n\n  useEffect(() => {\n    setElements([...nodeActions, ...nodeQuestions, ...nodeEdges])\n  }, [nodeActions, nodeQuestions, nodeEdges])\n  useEffect(() => {\n    if (reactflowInstance && elements.length > 0) {\n      reactflowInstance.fitView()\n    }\n  }, [reactflowInstance, elements.length])\n  const onElementsRemove = useCallback(\n    (elementsToRemove) =>\n      setElements((els) => removeElements(elementsToRemove, els)),\n    []\n  )\n  const onConnect = useCallback(\n    (params) =>\n      setElements((els) =>\n        addEdge({ ...params, animated: true, style: { stroke: '#fff' } }, els)\n      ),\n    []\n  )\n  const onLoad = useCallback(\n    (rfi) => {\n      if (!reactflowInstance) {\n        setReactflowInstance(rfi)\n        console.log('flow loaded:', rfi)\n      }\n    },\n    [reactflowInstance]\n  )\n  console.log('reactflowInstance => ', reactflowInstance)\n  return (\n    <div>\n      <ReactFlow\n        elements={elements}\n        onElementClick={onElementClick}\n        onElementsRemove={onElementsRemove}\n        onConnect={onConnect}\n        onNodeDragStop={onNodeDragStop}\n        style={{ background: '#e5e5e5', height: 'calc(100vh - 60px)' }}\n        onLoad={onLoad}\n        nodeTypes={nodeTypes}\n        connectionLineStyle={connectionLineStyle}\n        snapToGrid={true}\n        snapGrid={snapGrid}\n        minZoom={0.1}\n        maxZoom={2}\n        defaultZoom={2}\n      >\n        <Controls />\n        {/* <MiniMap\n          nodeColor={(node) => {\n            switch (node.type) {\n              case 'finish':\n                return '#f5644a'\n              case 'start':\n                return '#00c853'\n              case 'question':\n                return 'rgb(7, 145, 229)'\n              default:\n                return '#eee'\n            }\n          }}\n          nodeStrokeWidth={3}\n          nodeBorderRadius={6}\n        /> */}\n      </ReactFlow>\n      <QuestionForm\n        handleClose={() => setVisibleQuestion(false)}\n        open={visibleQuestion}\n        node={currentNode}\n      />\n    </div>\n  )\n}\nexport default NodeFlow\n","/home/doni/projects/reactjs_diagrams/src/actions/nodeActions/nodeActionTypes.js",[],"/home/doni/projects/reactjs_diagrams/src/reducers/actionReducer.js",[],"/home/doni/projects/reactjs_diagrams/src/actions/actionActions/actionActionTypes.js",[],"/home/doni/projects/reactjs_diagrams/src/reducers/questionReducer.js",[],"/home/doni/projects/reactjs_diagrams/src/reducers/relationReducer.js",[],"/home/doni/projects/reactjs_diagrams/src/actions/relationActions/relationActionTypes.js",[],"/home/doni/projects/reactjs_diagrams/src/components/nodeTypes/finishNode.js",[],"/home/doni/projects/reactjs_diagrams/src/components/nodeTypes/conditionNode.js",[],"/home/doni/projects/reactjs_diagrams/src/components/sidebar/cards/contentCards.jsx",[],"/home/doni/projects/reactjs_diagrams/src/components/nodeTypes/textQuestion.js",[],"/home/doni/projects/reactjs_diagrams/src/store.js",[],"/home/doni/projects/reactjs_diagrams/src/components/navbar/navbar.jsx",[],"/home/doni/projects/reactjs_diagrams/src/components/nodeTypes/index.js",[],"/home/doni/projects/reactjs_diagrams/src/containers/home.js",[],"/home/doni/projects/reactjs_diagrams/src/reportWebVitals.js",[],"/home/doni/projects/reactjs_diagrams/src/components/questionForm.js",[],"/home/doni/projects/reactjs_diagrams/src/components/nodeElements.js",[],"/home/doni/projects/reactjs_diagrams/src/containers/example.js",[],"/home/doni/projects/reactjs_diagrams/src/reducers/drawerReducer.js",[],"/home/doni/projects/reactjs_diagrams/src/actions/drawerActions/drawerActionTypes.js",[],"/home/doni/projects/reactjs_diagrams/src/components/nodeTypePopover.js",[],"/home/doni/projects/reactjs_diagrams/src/components/forms/textForm.js",[],{"ruleId":"143","replacedBy":"144"},{"ruleId":"145","replacedBy":"146"},{"ruleId":"